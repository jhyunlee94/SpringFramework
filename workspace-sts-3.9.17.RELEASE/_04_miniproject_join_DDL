CREATE TABLE KOCTION_USER (
	USER_ID VARCHAR(100) PRIMARY KEY COMMENT '아이디',
	USER_PW VARCHAR(100) NOT NULL COMMENT '비밀번호',
	USER_NM VARCHAR(100) NOT NULL COMMENT '이름',
	USER_TEL VARHAR(100) COMMENT '전화번호',
	USER_AC VARCHAR(100) COMMENT '계좌번호',
	USER_AD VARCHAR(100) COMMENT '주소',
	USER_ADD VARCHAR(100) COMMENT '상세주소',
	USER_MAIL VARCHAR(100) NOT NULL COMMENT '이메일'
);